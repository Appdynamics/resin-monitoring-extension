    <monitor>
        <name>ResinMonitor</name>
        <type>managed</type>
        <description>Resin monitor</description>
        <monitor-configuration></monitor-configuration>
        <monitor-run-task>
                <execution-style>periodic</execution-style>
                <execution-frequency-in-seconds>60</execution-frequency-in-seconds>
                <name>Resin Monitor Run Task</name>
                <display-name>Resin Monitor Task</display-name>
                <description>Resin Monitor Task</description>
                <type>java</type>
                <execution-timeout-in-secs>60</execution-timeout-in-secs>
                <task-arguments>
                    <!-- config file-->
                    <argument name="config-file" is-required="true" default-value="monitors/ResinMonitor/config.yml" />
                </task-arguments>
                <java-task>
                        <classpath>resin-monitoring-extension.jar</classpath>
                        <impl-class>com.appdynamics.monitors.resin.ResinMonitor</impl-class>
                </java-task>
        </monitor-run-task>
</monitor>